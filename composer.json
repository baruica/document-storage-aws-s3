{
    "name": "baruica/document-storage-aws-s3",
    "type": "library",
    "description": "Provides an aws s3 adapter for document-storage",
    "keywords": ["aws", "document-storage", "s3"],
    "homepage": "https://github.com/baruica/document-storage-aws-s3",
    "license": "Apache-2.0",
    "autoload": {
    "psr-4": { "DocumentStorage\\": "src/DocumentStorage" },
        "exclude-from-classmap": [ "tests" ]
    },
    "autoload-dev": {
        "psr-4": { "DocumentStorage\\": "tests/DocumentStorage" }
    },
    "require": {
        "php": "^7.0",
        "baruica/document-storage": "^0.6.1",
        "aws/aws-sdk-php" : "^3.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "phpspec/phpspec": "^3.1"
    }
}
